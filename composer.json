{
    "name": "hgod/hgwputils",
    "description": "Classes para criar custom-post-types, taxonomies, loads e admins pages no wordpress",
    "type": "library",
    "authors": [{
        "name": "Henrique Godinho",
        "email": "ola@hgod.in"
    }],
    "require": {},
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/phpcompatibility-wp": "*",
        "wp-coding-standards/wpcs": "^0.14.0",
        "automattic/vipwpcs": "*", 
        "wp-cli/wp-cli-bundle": "*"
    },
    "scripts": {
        "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs --standard=WordPress --colors",
        "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf --standard=WordPress --colors"
    },
    "autoload": {
        "psr-4": {
            "HGWP_Utils\\": "src/"
        }
    }
}
